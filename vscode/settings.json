{
    "go.alternateTools": {
        "go": "${workspaceFolder}/.vscode/scripts/go-wrapper.sh",
        //"dlv": "${workspaceFolder}/.vscode/scripts/dlv-wrapper.sh",
    },
    "go.delveConfig": {
        "debugAdapter": "legacy",
    },
    "go.lintTool": "golangci-lint",
    "go.lintFlags": ["--fast"],
    "editor.rulers": [
        100
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "git.enabled": false,
    "git.autofetch": false,
}
